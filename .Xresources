URxvt.internalBorder: 20
URxvt.scrollBar: false

URxvt.iso14755: false
URxvt.iso14755_52: false

URxvt*background:                     #000000
URxvt*foreground:                     #e0e0e0
URxvt*cursorColor:                    #e0e0e0

URxvt*scrollColor:                    #e0e0e0
