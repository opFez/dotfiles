# .profile

# variables
export PATH="$PATH:$HOME/bin:$HOME/.local/bin:$HOME/.cargo/bin"
export EDITOR="/bin/e"
export PAGER="less"
export BROWSER="firefox"
export QT_AUTO_SCREEN_SCALE_FACTOR=1
export GDK_SCALE=2
export TERMINAL="alacritty"
export GOPATH="$HOME/dev/go"

export PATH="$HOME/.cargo/bin:$PATH"
